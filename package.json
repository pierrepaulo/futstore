{
  "name": "futstore",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently -n WEB,API -c auto \"npm:dev:web\" \"npm:dev:api\"",
    "dev:web": "next dev -p 3000",
    "dev:api": "tsx watch --env-file=.env src/server.ts",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "db:seed": "prisma db seed"
  },
  "dependencies": {
    "@prisma/client": "^6.16.2",
    "axios": "^1.12.0",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "next": "15.5.3",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "stripe": "^18.5.0",
    "uuid": "^13.0.0",
    "zod": "^4.1.9",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/node": "^20.19.17",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "concurrently": "^9.0.0",
    "eslint": "^9",
    "eslint-config-next": "15.5.3",
    "prisma": "^6.16.2",
    "tailwindcss": "^4",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}
